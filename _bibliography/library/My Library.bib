@inproceedings{goyalBoundedCommunicationLeakageResilience2016,
  title = {Bounded-{{Communication Leakage Resilience}} via {{Parity-Resilient Circuits}}},
  booktitle = {2016 {{IEEE}} 57th {{Annual Symposium}} on {{Foundations}} of {{Computer Science}} ({{FOCS}})},
  author = {Goyal, Vipul and Ishai, Yuval and Maji, Hemanta K. and Sahai, Amit and Sherstov, Alexander A.},
