@misc{kortenStrongerCellProbe2025,
  title = {Stronger {{Cell Probe Lower Bounds}} via {{Local PRGs}}},
  author = {Korten, Oliver and Pitassi, Toniann and Impagliazzo, Russell},
  year = {2025},
