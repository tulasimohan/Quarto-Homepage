  langid = {english},
  keywords = {cell probe model,CSP refutation,data structures,delete,NC0 Cryptography,range avoidance},
  file = {/Users/tulasi/Zotero/storage/EEGV28ZT/Korten et al. - 2025 - Stronger Cell Probe Lower Bounds via Local PRGs.pdf}
}

@inproceedings{larsenCellProbeComplexity2012,
